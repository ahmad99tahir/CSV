    "# Create a NumPy array\n",
    "data = np.array([[\"Name\", \"Age\", \"Country\"],\n",
    "                 [\"John Doe\", 30, \"United States\"],\n",
    "                 [\"Jane Doe\", 28, \"Canada\"]])\n",
    "# Save the array to a CSV file\n",
    "np.savetxt(\"output.csv\", data, delimiter=\",\", fmt=\"%s\")"
